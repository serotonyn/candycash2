{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": [
            "serve"
          ],
          "name": "pocketbase",
          "cmd": "pocketbase",
          "sidecar": true
        }
      ]
    }
  ]
}