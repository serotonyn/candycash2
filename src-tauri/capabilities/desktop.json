{
  "identifier": "desktop-capability",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "autostart:default",
    "updater:default",
    "process:default",
    "shell:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "dialog:allow-message"
  ]
}